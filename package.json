{
  "name": "oursayso",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@cloudinary/react": "^1.14.3",
    "@cloudinary/url-gen": "^1.21.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "cloudinary": "^2.7.0",
    "lucide-react": "^0.539.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run version:build-info && react-scripts build",
    "build:production": "npm run version:increment && npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "version:status": "node ../version-utils.js status .",
    "version:increment": "node ../version-utils.js increment . patch",
    "version:increment:minor": "node ../version-utils.js increment . minor",
    "version:increment:major": "node ../version-utils.js increment . major",
    "version:build-info": "node ../version-utils.js build-info .",
    "version:release": "node ../version-utils.js release . patch",
    "version:release:minor": "node ../version-utils.js release . minor 'Minor release'",
    "version:release:major": "node ../version-utils.js release . major 'Major release'",
    "deploy": "npm run version:release && npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
